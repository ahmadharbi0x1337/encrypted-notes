# .env.example
# Rename this to .env and filll in your actual secrets before running the app.

# Flask secret key for session security
SECRET_KEY=your-secret-key-here

# Fernet encryption key (generate with `python utils/generate_master_key.py`)
FERNET_KEY=your-generated-fernet-key-here

# Database URI (leave it default for local SQLite)
DATABASE_URL=sqlite:///site.db